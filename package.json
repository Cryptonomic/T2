    "react-number-format": "^5.0.1",
    "react-pdf": "^5.7.2",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "styled-components": "^5.3.6",
    "throttle-debounce": "^5.0.0",
    "tss-react": "^4.4.1",
    "zxcvbn": "^4.4.2"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  },
  "overrides": {
    "electron-rebuild": {
      "node-abi": "3.28.0"
    }
  }
}
